<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginVertical="90dp"
    tools:context="com.example.g1cafetinues.Mantenimiento_Local.MantenimientoLocalFragment">


    <Button
        android:id="@+id/btn_AgregarLocal"
        android:layout_width="match_parent"
        android:layout_marginBottom="30dp"
        android:layout_height="wrap_content"
        android:text="agregar Local" />

    <Button
        android:id="@+id/btn_actualizarLocal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:text="Actualizar Local" />

    <Button
        android:id="@+id/btn_consultarLocal"
        android:layout_width="match_parent"
        android:layout_marginBottom="30dp"
        android:layout_height="wrap_content"
        android:text="Consultar Local" />

    <Button
        android:id="@+id/btn_eliminaLocal"
        android:layout_width="match_parent"
        android:layout_marginBottom="30dp"
        android:layout_height="wrap_content"
        android:text="Eliminar Local" />
</LinearLayout>
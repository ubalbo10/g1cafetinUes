<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginVertical="90dp"
    tools:context="com.example.g1cafetinues.Mantenimiento_Local.MantenimientoLocalFragment">


    <Button
        android:id="@+id/btn_AgregarCambioPrecio"
        android:layout_width="match_parent"
        android:layout_marginBottom="30dp"
        android:layout_height="wrap_content"
        android:text="agregar CambioPrecio" />

    <Button
        android:id="@+id/btn_actualizarCambioPrecio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:text="Actualizar CambioPrecio" />

    <Button
        android:id="@+id/btn_consultarCambioPrecio"
        android:layout_width="match_parent"
        android:layout_marginBottom="30dp"
        android:layout_height="wrap_content"
        android:text="Consultar CambioPrecio" />

    <Button
        android:id="@+id/btn_eliminaCambioPrecio"
        android:layout_width="match_parent"
        android:layout_marginBottom="30dp"
        android:layout_height="wrap_content"
        android:text="Eliminar CambioPrecio" />
</LinearLayout>